{
	"name": "Beautiful Theme",
	"colors": {
		
		"foreground": "#ECF4F7",
		"focusBorder": "#e94388",
		"selection.background": "#d483a5",

		"window.activeBorder": "#cc0055",
		"window.inactiveBorder": "#dbbfcc",

		"editor.background": "#471f29",
		"editor.foreground": "#ECF4F7",
		"editorWidget.background": "#9e4e70",
		"widget.shadow": "#9e4e70",

		"editorGroupHeader.tabsBackground": "#492c38",
		"tab.inactiveBackground": "#300b1b",
		"tab.hoverBackground": "#7a4c5f",
		"editorGroup.border": "#7a4c5f",

		"activityBar.background": "#793f57",
		"activityBar.inactiveForeground": "#dbbfcc",
		"badge.background": "#cc0055",
		"activityBarBadge.background": "#cc0055",
		"activityBar.activeBorder": "#e94388",

		"sideBar.background": "#492c38",
		"sideBarSectionHeader.background": "#7a4c5f",

		"inputOption.activeBackground": "#e94388",

		"scrollbarSlider.activeBackground": "#cc0055",
		"scrollbarSlider.hoverBackground": "#991e51",
		"scrollbarSlider.background": "#8a0a3f",

		"statusBar.background": "#cc0055",
		"statusBar.debuggingBackground": "#C3E88D",

		"button.background": "#cc0055",
		"button.hoverBackground": "#7a4c5f",
		"input.background": "#7a4c5f",

		"panel.background": "#3c1526",

		"textLink.foreground": "#cc0055",
		"textLink.activeForeground": "#C3E88D",

	},
	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#cc0055"
			}
		},
		{
			"name": "Variable",
			"scope": [
				"source",
			],
			"settings": {
				"foreground": "#f7adbf"
			}
		},
		{
			"name": "Keyword",
			"scope": [
				"keyword.control",
				"constant.other.color",
				"keyword.other.template",
				"keyword.other.substitution",
				"storage.type",
				"storage.modifier",
				"keyword.operator.logical"
			],
			"settings": {
				"foreground": "#e94388"
			}
		},
		{
			"name": "Operator, Punctuation",
			"scope": [
				"keyword.operator",
				"keyword.operator.comparison",
				"punctuation.separator",
				"punctuation.definition",
				"punctuation.section",
			],
			"settings": {
				"foreground": "#ecf4f7"
			}
		},
		{
			"name": "Function, Special Method",
			"scope": [
				"entity.name.function",
				"variable.function",
				"support.function",
				"keyword.other.special-method"
			],
			"settings": {
				"foreground": "#C3E88D"
			}
		},
		{
			"name": "Function Parameter",
			"scope": "variable.parameter.function",
			"settings": {
				"foreground": "#fb5e78"
			}
		},
		{
			"name": "Number, Constant",
			"scope": [
				"constant.numeric",
				"constant.language",
				"constant.character",
				"constant.escape",
				"support.constant",
				"support.variable"
			],
			"settings": {
				"foreground": "#6688f5"
			}
		},
		{
			"name": "String",
			"scope": [
				"string",
				"punctuation.definition.string"
			],
			"settings": {
				"foreground": "#F78C6C"
			}
		},
		{
			"name": "Class, Support",
			"scope": [
				"entity.name",
				"support.type",
				"support.class",
				"entity.other.inherited-class",
				"punctuation.definition.decorator"
			],
			"settings": {
				"foreground": "#be06ce"
			}
		},
		{
			"name": "URL",
			"scope": [
				"*url*",
				"*link*",
				"*uri*"
			],
			"settings": {
				"fontStyle": "underline"
			}
		},
	],
}


